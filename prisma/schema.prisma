generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Group {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  tags        Json
  created_by  String
  updated_by  String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
  status      Int      @default(1)
}

model Channel {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  tags        Json
  created_by  String
  updated_by  String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
  status      Int      @default(1)
}
